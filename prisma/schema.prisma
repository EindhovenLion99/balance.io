generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mysql"
}

model Wallet {
  id            Int      @id @default(autoincrement())
  name          String   @db.VarChar(255)
  initialAmount Float
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
